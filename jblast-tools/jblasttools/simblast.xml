<tool id="jb_simblast" name="Sim Blastn" version="1.0.0">
  <description>Simulate Blastn</description>
  <command>jblast-simblast -i $input1 -o $output -b blastn-sample-with-hits.blastxml</command>
  <inputs>
    <param format="fasta" name="input1" type="data" label="fasta"/>
  </inputs>
  <outputs>
    <data format="blastxml" name="output" label="blastn sim - blastxml"/>
  </outputs>

  <help>
AUTHOR: eyao                               
  </help>

</tool>